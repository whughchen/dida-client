.container{
    background: #f4f4f4;
    width: 100%;
    height: auto;
    min-height: 100%;
    overflow: hidden;
}
.banner {
  width: 750rpx;
  height: 417rpx;
}

.banner image {
  width: 100%;
  height: 417rpx;
}

.m-menu {
  display: flex;
  height: 181rpx;
  width: 750rpx;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
}

.m-menu .item {
  flex: 1;
  display: block;
  padding: 20rpx 0;
}

.m-menu image {
  display: block;
  width: 58rpx;
  height: 58rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}

.m-menu text {
  display: block;
  font-size: 24rpx;
  text-align: center;
  margin: 0 auto;
  line-height: 1;
  color: #333;
}

.a-section {
  width: 750rpx;
  height: auto;
  overflow: hidden;
  background: #fff;
  color: #333;
  margin-top: 8rpx;
}

.a-section .h {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  height: 130rpx;
}

.a-section .h .txt {
  padding-right: 30rpx;
  background-size: 16.656rpx 27rpx;
  display: inline-block;
  height: 36rpx;
  font-size: 33rpx;
  line-height: 36rpx;
}

.a-brand .b {
  width: 750rpx;
  height: auto;
  overflow: hidden;
  position: relative;
}

.a-brand .wrap {
  position: relative;
}

.a-brand .img {
  position: absolute;
  left: 0;
  top: 0;
}

.a-brand .mt {
  position: absolute;
  z-index: 20;
  padding: 27rpx 31rpx;
  left: 0;
  top: 0;
}

.a-brand .mt .brand {
  display: block;
  font-size: 33rpx;
  height: 43rpx;
  color: #333;
}

.a-brand .mt .price, .a-brand .mt .unit {
  font-size: 25rpx;
  color: #999;
}


.a-brand .mt .brand11 {
display: block;
  font-size: 30rpx;
  height: 30rpx;
  color: #333;
  
  
  position: relative;
  z-index: 20;
  padding: 150rpx 60rpx;
  left: 0;
  top: 0;
}
.a-brand  .text1 {
  display: -webkit-inline-box;
  font-size: 25rpx;
  height: 25rpx;
  color: #999;
  
  position: relative;
  z-index: 20;
  padding: 210rpx 10rpx;
  left: 20;
  top: 0;
}
.a-brand .secondline .text2 {
  display: -webkit-inline-box;
  font-size: 25rpx;
  height: 25rpx;
  color: #999;
  
  position: relative;
  z-index: 20;
  padding: 210rpx 10rpx;
  left: 20;
  top: 0;
}

.a-brand .item-1 {
  float: left;
  width: 375rpx;
  height: 252rpx;
  overflow: hidden;
  border-top: 1rpx solid #fff;
  margin-left: 1rpx;
  border: 1px solid #000000;
}
.a-brand .vehicle {
  float: left;
  width: 375rpx;
  height: 282rpx;
  overflow: hidden;
  border-top: 1rpx solid #fff;
  margin-left: 1px;
}
.vehicle-choosed {
  float: left;
  width: 49%;
  height: 282rpx;
  overflow: hidden;
  border-top: 1rpx solid #fff;
  margin-left: 1px;
  color: #999;
  border-radius:4px;
  z-index: 21;

  border:solid 1px #000000; 
  border-color: red;
}

.vehicle-unchoosed {
  float: left;
  width: 49%;
  height: 282rpx;
  overflow: hidden;
  border-top: 1rpx solid #fff;
  margin-left: 1px;
  color: #999;
  border-radius:2px;
    z-index: 21;
}

.on-choosed{
  border:solid 1px #000000; 
  border-radius:2px;
  z-index: 21;
  border-color: green;
}

.a-brand .item-1:nth-child(2n+1){
  margin-left: 0;
  width: 374rpx;
}

.a-brand .item-1 .img {
  width: 370rpx;
  height: 253rpx;
}

.a-new .b {
